{
  "name": "mkashima-portfolio-2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export && echo mochaup.com > out/CNAME && echo > out/.nojekyll",
    "start": "next start",
    "lint": "next lint",
    "clean": "rd /s /q node_modules",
    "clean_unix": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "reinstall_unix": "npm run clean_unix && npm install",
    "deploy": "next build && cd out && git add . && git commit -m \"deploying to gh-pages\" && git push -u origin gh-pages"
  },
  "dependencies": {
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "babel-plugin-transform-define": "^2.0.1",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7"
  }
}
